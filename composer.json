{
  "name": "tourze/symfony-aop-async-bundle",
  "description": "Async AOP Bundle for Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/symfony-aop-bundle": "~0.0.4",
    "tourze/async-service-call-bundle": "0.0.*",
    "symfony/messenger": "^6.4",
    "psr/log": "^3|^2|^1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopAsyncBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\AopAsyncBundle\\Tests\\": "tests"
    }
  }
}