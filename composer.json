{
  "name": "tourze/symfony-aop-async-bundle",
  "description": "Async AOP Bundle for Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/async-service-call-bundle": "0.0.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-aop-bundle": "~0.0.4",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopAsyncBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\AopAsyncBundle\\Tests\\": "tests"
    }
  }
}